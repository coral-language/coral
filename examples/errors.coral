def bad_function(x: int) -> int:
    if x > 0:
        return x

def test_unreachable():
    return 5
    print("This code is unreachable")

result = test_unreachable()
print(f"Result: {result}")

def test_unreachable_pattern(value):
    match value:
        case x:
            return "first"
        case _:
            return "unreachable"
        case 42:
            return "also unreachable"

def test_wildcard_in_or(value):
    match value:
        case 1 | 2 | _:
            return "bad"

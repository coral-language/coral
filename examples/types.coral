module main

type User = {
  id: Int,
  name: String,
  email: String,
  active: Bool
}

fn createUser(name: String, email: String) -> User do
  return User {
    id: 1,
    name: name,
    email: email,
    active: true
  }
end

fn main() do
  user = createUser("Alice", "alice@example.com")
  print(user.name)
  return 0
end
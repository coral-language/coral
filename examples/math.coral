fn add(x: Int, y: Int) -> Int do
  return x + y
end

fn multiply(x: Int, y: Int) -> Int do
  return x * y
end

fn factorial(n: Int) -> Int do
  if n <= 1 do
    return 1
  else
    return n * factorial(n - 1)
  end
end

a = 10
b = 20
sum = add(a, b)
product = multiply(a, b)

println("Sum: {sum}")
println("Product: {product}")

fact = factorial(5)
println("Factorial of 5: {fact}")